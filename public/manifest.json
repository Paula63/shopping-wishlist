{
  "manifest_version": 3,
  "name": "Shopping Wishlist",
  "version": "1.3.7",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"],
      "css": ["stylesheet/content.css"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background-wrapper.js"
  },
  "host_permissions": [ 
    "<all_urls>"
  ],
  "action": {
    "default_icon": "images/shopping-bag.png",
    "default_popup": "interface/popup.html",
    "default_title": "Shopping Assistant"
  }
}
